/* activity/mind/result.wxss */
page{
  background: url(https://ssl.kdyoushu.com/applet/mind_test/an_result_bg.png) repeat-y center;
  background-size: cover
}
.main{
  width: 100%;
  padding-bottom: 30rpx;
}
.banner{
  width: 100%;
  margin-top: -30rpx;
}
.title{
  margin: -120rpx auto 0;
  text-align: center;
  height: 130rpx;
  line-height: 130rpx;
  background: url(https://ssl.kdyoushu.com/applet/mind_test/at_rs_frame.png) no-repeat center;
  background-size: contain;
  z-index: 100;
  position: relative;
  color: #917466;
  font-size: 38rpx
}
.name{
  font-size: 45rpx;
  font-weight: bold;
  position: relative;
  top: -2rpx;
}
.img-w{
  width: 30vw;
  height: 30vw;
  margin:  0 auto 0 ;
  background: url(https://ssl.kdyoushu.com/applet/mind_test/at_hd_frame.png) no-repeat center;
  padding: 20rpx;
  padding-top: 40rpx;
  background-size: contain;
}
.img{
  width: 100%;
  margin-top: -10rpx;
  border: 5rpx solid #d7ccb4;
  box-sizing: border-box;
}
.analyze{
  padding: 30rpx 40rpx;
  padding-top: 10rpx
}
.analyze-text{
  color: #000;
  font-size: 16px;
  margin-bottom: 5rpx
}
.desc{
  color: #544434;
  line-height: 1.4;
  font-size: 14px;
}
.share-w{
  display: flex;
  justify-content: space-around;
}

.share-w .view1{
  margin-top: 10rpx;
  width: 40%;
  height:  10vw;
  line-height: 10vw;
  text-align: center;
  color: #fff;
  font-size: 35rpx;
  background: url(https://ssl.kdyoushu.com/applet/mind_test/at_test_btn.png) no-repeat center;
  background-size: contain;
  padding: 0;
  border: 0;
  margin: 0;
}
.share-w .view1{
  background-image: url(data:image/png;base64,)
}
.share-w .btn{
  background-image: url(data:image/png;base64,)
}
button::after{
  content: none
}
.bottom{
  color: #333;
  margin-top: 20rpx;
  padding: 30rpx;
}
.bottom navigator {
  display: block
}
.bottom .text{
  color: #666;
  font-size: 30rpx;
  text-align: center;
  width: 26vw;
}
.row{
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
  align-items: top;
  margin-bottom: 20rpx;
}
.bottom image{
  width: 22vw;
  display: block;
  margin: 0 auto;
}
.bottom-tit{
  color: #999;
  font-size: 30rpx;
  padding-left: 5vw;
}

